# Staging Environment Configuration
NODE_ENV=staging
VITE_APP_ENV=staging
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-staging-anon-key

# PayU Payment Gateway Configuration (Staging/Test)
VITE_PAYU_KEY=your_staging_payu_key
VITE_PAYU_SALT=your_staging_payu_salt
VITE_PAYU_SUCCESS_URL=https://staging.yourdomain.com/payment/success
VITE_PAYU_FAILURE_URL=https://staging.yourdomain.com/payment/failed
VITE_PAYU_CANCEL_URL=https://staging.yourdomain.com/pricing

# API Keys for staging (use test/sandbox keys)
ANTHROPIC_API_KEY=sk-ant-api03-staging-key
PERPLEXITY_API_KEY=pplx-staging-key
OPENAI_API_KEY=sk-proj-staging-key

# Admin configuration
VITE_ADMIN_EMAILS=admin@company.com,staging-admin@company.com

# Analytics (staging)
VITE_ANALYTICS_ID=staging-analytics-id

# Feature flags for staging
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false